<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    tools:context=".ResetDataStudent">

    <Button
        android:id="@+id/back_resetDataStudent"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/rounded_button"
        android:bufferType="normal"
        android:fontFamily="@font/cabin"
        android:selectAllOnFocus="false"
        android:text="@string/back_to_settings_options_ResetDataStudent"
        android:textAlignment="center"
        android:textColor="#050000"
        android:textColorHighlight="#050000"
        android:textColorLink="#040000"
        android:textSize="14sp"
        android:typeface="normal"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resetIndexNumber_resetDataStudent" />

    <Button
        android:id="@+id/resetIndexNumber_resetDataStudent"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/rounded_button"
        android:bufferType="normal"
        android:fontFamily="@font/cabin"
        android:selectAllOnFocus="false"
        android:text="@string/reset_index_number_ResetDataStudent"
        android:textAlignment="center"
        android:textColor="#050000"
        android:textColorHighlight="#050000"
        android:textColorLink="#040000"
        android:textSize="14sp"
        android:typeface="normal"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resetDegreeAndSemester" />

    <Button
        android:id="@+id/resetDegreeAndSemester"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/rounded_button"
        android:bufferType="normal"
        android:fontFamily="@font/cabin"
        android:selectAllOnFocus="false"
        android:text="@string/reset_degree_and_semester_ResetDataStudent"
        android:textAlignment="center"
        android:textColor="#050000"
        android:textColorHighlight="#050000"
        android:textColorLink="#040000"
        android:textSize="14sp"
        android:typeface="normal"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resetFacultyAndField_resetDataStudent" />

    <Button
        android:id="@+id/resetFacultyAndField_resetDataStudent"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/rounded_button"
        android:bufferType="normal"
        android:fontFamily="@font/cabin"
        android:selectAllOnFocus="false"
        android:text="@string/reset_faculty_and_field_ResetDataStudent"
        android:textAlignment="center"
        android:textColor="#050000"
        android:textColorHighlight="#050000"
        android:textColorLink="#040000"
        android:textSize="14sp"
        android:typeface="normal"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_settingPanel2" />

    <TextView
        android:id="@+id/textview_settingPanel2"
        style="@android:style/Widget.TextView"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_margin="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:height="50dp"
        android:fontFamily="@font/cabin"
        android:gravity="center"
        android:orientation="horizontal"
        android:text="@string/settings_panel_ResetDataStudent"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="30sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>